{
  "name": "symbol-autocomplete",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Symbol Autocomplete",
    "manifest": "src/manifest.json",
    "main": "symbol-autocomplete.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  },
  "author": "Ariel Verber <opiedesigns@gmail.com>",
  "dependencies": {
    "sketch-module-web-view": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opiex/symbol-autocomplete"
  }
}
